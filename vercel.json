{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "crons": [
      {
        "path": "/api/deleteExpiredSharedAgreements",
        "schedule": "0 1 * * *"
      },
      {
        "path": "/api/sendStatusUpdateEmails",
        "schedule": "0 */6 * * *"
      }
    ]
}